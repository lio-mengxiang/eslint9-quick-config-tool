{
  "name": "@mx-design/config-eslint9",
  "version": "1.0.3",
  "description": "update eslint 9.x configuration",
  "repository": "https://github.com/lio-mengxiang/eslint9-quick-config-tool",
  "keywords": [
    "eslint9",
    "eslint migrate to 9",
    "eslint migrate",
    "eslint react",
    "eslint vue",
    "eslint next",
    "eslint nextjs",
    "eslint typescript",
    "eslint9 javascript",
    "eslint9 react",
    "eslint9 vue",
    "eslint9 next",
    "eslint9 nextjs",
    "eslint9 typescript",
    "eslint9 javascript"
  ],
  "bin": {
    "config-eslint9": "./bin/index.js"
  },
  "engines": {
    "node": ">=18.2"
  },
  "scripts": {
    "build": "rimraf ./esm && rollup -c",
    "clean": "rimraf {esm,types} --glob",
    "dev": "rollup -c -w",
    "clean-nm": "rm -rf node_modules",
    "prepare": "husky",
    "commit": "git add -A && git-cz",
    "change-log": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@clack/prompts": "0.7.0",
    "chalk": "5.3.0",
    "commander": "12.1.0",
    "ejs": "3.1.10"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.17.0",
    "@rollup/plugin-json": "6.1.0",
    "@types/ejs": "3.1.5",
    "@types/node": "^22.8.4",
    "@types/react": "^19.0.2",
    "commitizen": "4.3.1",
    "commitlint-config-cz": "0.13.3",
    "conventional-changelog-cli": "5.0.0",
    "cz-customizable": "7.2.1",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import-x": "4.6.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.16",
    "eslint-plugin-tailwindcss": "3.17.5",
    "eslint-plugin-vue": "9.32.0",
    "globals": "15.14.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "rollup": "4.24.0",
    "rollup-plugin-alias": "2.2.0",
    "rollup-plugin-esbuild": "6.1.1",
    "rollup-plugin-node-externals": "7.1.3",
    "typescript": "5.6.2",
    "typescript-eslint": "8.18.1"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx": [
      "eslint --fix",
      "prettier -w"
    ],
    "*.{md,json}": [
      "prettier -w"
    ]
  },
  "files": [
    "bin",
    "esm"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
